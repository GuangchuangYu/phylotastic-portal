<% if @ra.nil? %>
  $('#ra-holder').html("<%= escape_javascript(render partial: 'con_files/content_error', locals: {f: @f}) %>");
<% else %>
  $('#ra-holder').html("<%= escape_javascript(render partial: 'content',
                          locals: { resolved_names: @resolved_names,
                                    unresolved:     @unresolved,
                                    name:           @f.name, 
                                    ra:             @ra} ) %>");                                                                
<% end %>